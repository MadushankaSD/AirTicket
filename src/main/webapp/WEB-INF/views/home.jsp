<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<!DOCTYPE html>
<html>
<head>
    <title>Air Ticket Booking</title>
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.2/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="${pageContext.request.contextPath}/resources/css/style.css" />
</head>
<body>

<jsp:include page="header.jsp" />
<!-- Hero Section -->
<section class="hero">
    <div class="container">
        <h1 class="display-4">Book Your Next Flight</h1>
        <p class="lead">Find the best deals on domestic and international flights</p>
    </div>
</section>

<!-- Search Form -->
<div class="container search-box">
    <form action="${pageContext.request.contextPath}/searchFlights" method="get" class="row g-3">
        <div class="col-md-4">
            <label for="fromCity" class="form-label">From</label>
            <select class="form-select" id="fromCity" required name="fromCity">
                <option value="">-- Select Departure City --</option>
                <c:forEach var="city" items="${cities}">
                    <option value="${city.name}">${city.name}</option>
                </c:forEach>
            </select>
        </div>
        <div class="col-md-4">
            <label for="toCity" class="form-label">To</label>
            <select class="form-select" id="toCity" name="toCity" required disabled>
                <option value="">-- Select Destination City --</option>
                <c:forEach var="city" items="${cities}">
                    <option value="${city.name}">${city.name}</option>
                </c:forEach>
            </select>
        </div>
        <div class="col-md-4">
            <label for="departureDate" class="form-label">Departure Date</label>
            <input type="date" class="form-control" id="departureDate" required name="departureDate">
        </div>
        <div class="col-12 text-center mt-3">
            <button type="submit" class="btn btn-primary btn-lg">Search Flights</button>
        </div>
    </form>
</div>

<!-- Popular Destinations -->
<div class="container mt-5">
    <h2 class="text-center mb-4">Popular Destinations</h2>
    <div class="row text-center">
        <div class="col-md-3">
            <img src="data:image/jpeg;base64," class="img-fluid rounded mb-2" alt="Paris">
            <p style="font-family: 'Arial Black', Gadget, sans-serif; font-size: 20px; color: #2c3e50;">
                Paris
            </p>
        </div>
        <div class="col-md-3">
            <img src="data:image/jpeg;base64," class="img-fluid rounded mb-2" alt="Tokyo">
            <p style="font-family: 'Arial Black', Gadget, sans-serif; font-size: 20px; color: #2c3e50;">
                Tokyo
            </p>
        </div>
        <div class="col-md-3">
            <img src="data:image/jpeg;base64," class="img-fluid rounded mb-2" alt="Dubai">
            <p style="font-family: 'Arial Black', Gadget, sans-serif; font-size: 20px; color: #2c3e50;">
                Dubai
            </p>

        </div>
        <div class="col-md-3">
            <img src="data:image/jpeg;base64," class="img-fluid rounded mb-2" alt="New York">
            <p style="font-family: 'Arial Black', Gadget, sans-serif; font-size: 20px; color: #2c3e50;">
                New York
            </p>
        </div>
    </div>
</div>

<!-- Footer -->
<div class="footer">
    <jsp:include page="footer.jsp" />
</div>

</body>
<script>
    const fromCity = document.getElementById('fromCity');
    const toCity = document.getElementById('toCity');

    // Save original options of 'toCity' to reset easily
    const originalToOptions = Array.from(toCity.options);

    fromCity.addEventListener('change', function() {
        const selectedFrom = this.value;

        if (selectedFrom) {
            // Enable 'toCity' dropdown
            toCity.disabled = false;

            // Clear all options from 'toCity'
            toCity.innerHTML = '';

            // Add back all original options except the selected fromCity
            originalToOptions.forEach(option => {
                if (option.value !== selectedFrom) {
                    toCity.appendChild(option.cloneNode(true));
                }
            });

            // Reset 'toCity' selection
            toCity.value = '';
        } else {
            // If no city selected, disable 'toCity' and reset options
            toCity.disabled = true;
            toCity.innerHTML = '';
            originalToOptions.forEach(option => {
                toCity.appendChild(option.cloneNode(true));
            });
            toCity.value = '';
        }
    });
</script>
</html>
